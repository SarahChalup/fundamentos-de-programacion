#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
void main()
{ float a,b,c,x,x2,d;
 cout<<"ingrese a"<<endl;
 cin>>a;
 cout<<"ingrese b"<<endl;
 cin>>b;
 cout<<"ingrese c"<<endl;
 cin>>c;
 d=b*b-4*a*c;
 if(d==0)
 { x=-b/2*a;
   cout<<"x es igual a:"<<endl;
   cout<<x<<endl;
   if(d>0)
   { x=-b-sqrt(b*b-4*a*c)/2*a;
     x2=-b+sqrt(b*b-4*a*c)/2*a;
	 cout<<"x1 es:"<<x<<endl;
	 cout<<"x2 es:"<<x2<<endl;}} else
	 {cout<<"el resultado es un numero imaginario"<<endl;}
	 getch();
}
