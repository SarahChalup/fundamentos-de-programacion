void main()
{
	float l,area;
	cout<<"ingresa el lado: "<<endl;
	cin>>l;
	area=l*l;
	cout<<"area: "<<endl;
	cout<<area<<endl;
	
	getch();

	
}
PSEINT
Proceso areadeuncuadrado
	Escribir 'insertar el lado:'
	Leer l
	Escribir 'area:'
	area<-(l*l)
	Escribir area
FinProceso
